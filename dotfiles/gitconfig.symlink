[hub]
  protocol = https
[user]
	name  = Craig Little
	email = craiglttl@gmail.com
[credential]
  helper = osxkeychain
[alias]
	co   = checkout
  ci   = commit
  st   = status
  br   = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[core]
	editor   = vim
	autocrlf = input
	safecrlf = true
[color]
  diff   = auto
  status = auto
  branch = auto
	ui     = true
